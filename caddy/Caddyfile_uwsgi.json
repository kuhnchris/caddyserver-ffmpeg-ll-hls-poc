{
  "logging": {
    "logs": {
      "default": {
        "level": "DEBUG",
        "writer": {
          "output": "stdout"
        }
      }
    }
  },
  "apps": {
    "http": {
      "grace_period": "60s",
      "servers": {
        "public": {
          "listen": [
            ":80"
          ],
          "routes": [
            {
              "match": [
                {
                  "host": [
                    "*"
                  ]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "flush_interval": -1,
                  "headers": {
                    "request": {
                      "set": {
                        "X-Server-Name": [
                          "test"
                        ]
                      }
                    }
                  },
                  "transport": {
                    "protocol": "http",
                    "keep_alive": {
                      "enabled": true
                    }
                  },
                  "upstreams": [
                    {
                      "dial": "uwsgi:8002"
                    }
                  ]
                }
              ],
              "terminal": true
            }
          ],
          "tls_connection_policies": [],
          "automatic_https": {
            "disable": true
          },
          "logs": {
            "default_logger_name": "default"
          }
        }
      }
    }
  }
}